.menu a { color:darkgreen;     font-family: 'Myriadprocond', serif; font-size:20px}

#.menuItem:hover >  span a{color:#f3f3f3;}

.menu {
height: 220px;
background: url(../../images/pics/1Untitled-1.png) !important;
    background-repeat: no-repeat;
    top: 250px;
padding-top: 39px;



border-radius: 4px;
  position: fixed;
/*  height: 100%;*/
  width: 185px;
/*  background: #2c3e50;
  background: rgba(29, 107, 93, 0.61); */
  background: rgba(87, 167, 105,0);

  transition:width 1s;
  overflow: hidden;
  font-family: 'Michroma', sans-serif;
  z-index: 99;
}


.menuItem span {
  position: relative;
  left:20px;
  top:0;
  transition:color 1s;
  color:rgba(255,255,255,.8);
  font-weight:500;
}


.menu:hover .menuItem  span {
 color:rgba(255,255,255,1);
}

.menuItem {
  position: relative;
  padding: 6px;
  transition:border .5s, background .2s;  
}

.menuItem:hover {
/*  background: #34495e; */
   background: #bcdcc2;
  cursor: pointer;
}

.uno {
  border-left:5px solid #16a085;
}

.uno:hover{
  border-left:20px solid #16a085;
}

.dos {
 border-left:5px solid #2980b9;
}

.dos:hover {
 border-left:20px solid #2980b9;
}

